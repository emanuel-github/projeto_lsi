<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="236.0" prefWidth="268.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projeto.lsi.controller.TelaLoginController">
   <children>
      <ImageView fx:id="imagemMenuPrincipal" fitHeight="237.0" fitWidth="269.0" opacity="0.37" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../imagens/ImagemMenuPrincipal.jpg" />
         </image>
      </ImageView>
      <Pane layoutX="-1.0" layoutY="-1.0" prefHeight="237.0" prefWidth="269.0">
         <children>
            <TextField fx:id="campoLogin" layoutX="60.0" layoutY="70.0" />
            <PasswordField fx:id="campoSenha" layoutX="60.0" layoutY="118.0" />
            <Button fx:id="botaoEntrar" layoutX="161.0" layoutY="163.0" mnemonicParsing="false" onAction="#entrarMenu" text="Entrar" />
            <Button fx:id="botaoCadastrar" layoutX="60.0" layoutY="163.0" mnemonicParsing="false" onAction="#cadastrarUsuario" text="Cadastrar" />
            <Label layoutX="112.0" layoutY="27.0" text="Login" />
         </children>
      </Pane>
   </children>
</AnchorPane>
